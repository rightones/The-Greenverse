{
    "presets": [
       "next/babel"
    ],
    "plugins": [
        [
            "styled-components",
            {
                "ssr": true,
                "displayName": true,
                "preprocess": false,
                "pure": true
            }
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ]
            }
        ]
    ],
    "env": {
        "production": {
            "plugins": [
                [
                    "styled-components",
                    {
                        "ssr": true,
                        "displayName": false,
                        "preprocess": false
                    }
                ]
            ]
        }
    }
}
